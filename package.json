{
  "name": "books-api",
  "version": "1.0.0",
  "description": "Books API",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=prod babel-node index.js",
    "test-integration": "NODE_ENV=test mocha --opts test/integration/mocha.opts test/integration/**/*.js",
    "test-unit": "NODE_ENV=test mocha --opts test/unit/mocha.opts test/unit/**/*.js",
    "test-contract": "NODE_ENV=test mocha --opts test/contracts/mocha.opts test/contracts/**/*.js",
    "test-all": "npm run test-integration && npm run test-unit && npm run test-contract",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix"
  },
  "keywords": [
    "node",
    "express",
    "es2015",
    "api",
    "test",
    "babel",
    "sequelize"
  ],
  "author": "Claudenir Freitas <claudenirfreitas.cf@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "http-status": "^0.2.3",
    "jwt-simple": "^0.5.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "sequelize": "^3.24.1",
    "sqlite3": "^3.1.4",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "joi": "^9.0.4",
    "joi-assert": "0.0.3",
    "mocha": "^3.0.2",
    "supertest": "^2.0.0",
    "testdouble": "^1.6.0"
  }
}
